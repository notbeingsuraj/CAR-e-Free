{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/auth/:match*",
            "destination": "/api/auth/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/client/index.html"
        }
    ]
}